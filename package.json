{
  "name": "jeffrpowell",
  "description": "Personal portfolio website for Jeff Powell",
  "version": "1.0.0",
  "author": "Jeff Powell",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --host 0.0.0.0 --port 4290 --live-reload",
    "start-prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --prod --port 4290 --live-reload",
    "build": "ng build --prod && cpx package.json ./dist/",
    "dev-build": "ng build && cpx package.json ./dist/",
    "lint": "ng lint",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json",
    "watch": "ng lint && ng build --aot --watch --verbose",
    "watch-prod": "ng lint && ng build --prod --watch --verbose",
    "format:check": "prettier --config ./.prettierrc --check \"./src/{app,environments,assets}/**/*.{ts,js,json,css,scss}\"",
    "format:fix": "prettier --config ./.prettierrc --write \"./src/{app,environments,assets}/**/*.{ts,js,json,css,scss}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/{app,environments,assets}/**/*.{ts,js,scss,css,json}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.7",
    "@angular/common": "~9.0.7",
    "@angular/compiler": "~9.0.7",
    "@angular/core": "~9.0.7",
    "@angular/forms": "~9.0.7",
    "@angular/localize": "~9.0.7",
    "@angular/platform-browser": "~9.0.7",
    "@angular/platform-browser-dynamic": "~9.0.7",
    "@angular/router": "~9.0.7",
    "@mdi/font": "~5.0.45",
    "@ng-bootstrap/ng-bootstrap": "~6.1.0",
    "@ng-select/ng-select": "~3.7.3",
    "bignumber.js": "~9.0.0",
    "bootstrap": "~4.4.1",
    "d3": "5.15.0",
    "jquery": "3.4.1",
    "lodash-es": "~4.17.15",
    "moment": "~2.24.0",
    "ngx-toastr": "~12.0.0",
    "popper.js": "1.16.1",
    "rxjs": "~6.5.4",
    "ts-key-enum": "^2.0.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.7",
    "@angular-devkit/schematics": "~9.0.7",
    "@angular/cli": "~9.0.7",
    "@angular/compiler-cli": "~9.0.7",
    "@types/d3": "5.7.2",
    "@types/lodash-es": "4.17.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "0.6.2",
    "codelyzer": "^5.1.2",
    "cpx": "~1.5.0",
    "fibers": "4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.1",
    "lint-staged": "^10.0.8",
    "node-sass": "4.13.1",
    "prettier": "1.18.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~3.7.5",
    "webpack-bundle-analyzer": "~3.6.1"
  }
}
