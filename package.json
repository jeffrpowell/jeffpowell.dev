{
  "name": "jeffrpowell",
  "description": "Personal portfolio website for Jeff Powell",
  "version": "1.0.0",
  "author": "Jeff Powell",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --host 0.0.0.0 --port 4290 --live-reload",
    "start-prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --configuration production --port 4290 --live-reload",
    "build": "ng build --configuration production && cpx package.json ./dist/",
    "dev-build": "ng build && cpx package.json ./dist/",
    "lint": "ng lint",
    "bundle-report": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/stats.json",
    "watch": "ng lint && ng build --aot --watch --verbose",
    "watch-prod": "ng lint && ng build --configuration production --watch --verbose",
    "format:check": "prettier --config ./.prettierrc --check \"./src/{app,environments,assets}/**/*.{ts,js,json,css,scss}\"",
    "format:fix": "prettier --config ./.prettierrc --write \"./src/{app,environments,assets}/**/*.{ts,js,json,css,scss}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/{app,environments,assets}/**/*.{ts,js,scss,css,json}": [
      "prettier --config ./.prettierrc --write"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.15",
    "@angular/common": "~12.2.15",
    "@angular/compiler": "~12.2.15",
    "@angular/core": "~12.2.15",
    "@angular/forms": "~12.2.15",
    "@angular/localize": "~12.2.15",
    "@angular/platform-browser": "~12.2.15",
    "@angular/platform-browser-dynamic": "~12.2.15",
    "@angular/router": "~12.2.15",
    "@mdi/font": "~6.5.95",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0",
    "@ng-select/ng-select": "^8.0.0",
    "bignumber.js": "~9.0.0",
    "bootstrap": "~4.4.1",
    "d3": "7.2.1",
    "jquery": "3.5.1",
    "lodash-es": "~4.17.15",
    "moment": "~2.29.1",
    "ngx-toastr": "~13.0.0",
    "popper.js": "1.16.1",
    "rxjs": "~6.5.4",
    "ts-key-enum": "^2.0.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1202.14",
    "@angular-devkit/build-angular": "~12.2.14",
    "@angular-devkit/core": "^12.2.14",
    "@angular-devkit/schematics": "~12.2.14",
    "@angular/cli": "~12.2.14",
    "@angular/compiler-cli": "~12.2.15",
    "@types/d3": "5.7.2",
    "@types/lodash-es": "4.17.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "1.0.0",
    "codelyzer": "^6.0.0",
    "cpx": "~1.5.0",
    "fibers": "4.0.2",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^3.0.1",
    "lint-staged": "^10.0.8",
    "node-sass": "4.13.1",
    "prettier": "1.18.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~4.3.5",
    "webpack": "^4.0.0",
    "webpack-bundle-analyzer": "~3.9.0"
  }
}